# Chunk 2
my.alpha <- 100
makeTransparent <- function(black, alpha = my.alpha){
newColor <- col2rgb(black)
apply(newColor, 2, function(curcoldata)
{rgb(red   = curcoldata[1],
green = curcoldata[2],
blue  = curcoldata[3],
alpha = alpha,
maxColorValue =  255)})
}
blue_transparent <- makeTransparent("blue")
red_transparent <- makeTransparent("red")
green_transparent <- makeTransparent("green")
